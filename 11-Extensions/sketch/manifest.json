{
    "manifest_version": 2,
    "name": "Sketch Extension",
    "version": "1.000",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "p5.dom.js",
                "p5.js",
                "sketch.js"
            ]
        }
    ],
    
    "browser_action": {
        "default_icon": "icon.png"
    }
}