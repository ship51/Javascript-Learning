
{
    "manifest_version": 2,
    "name": "Browser Pop-up",
    "version": "1.000",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    
    "browser_action":{
        "default_icon": "icon.png",
        "default_popup": "sketch/index.html",
        "default_title": "A popup will come here."
    }
}